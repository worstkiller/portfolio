{
  "name": "vikasdev",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://worstkiller.github.io/portfolio/",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "bulma": "^0.7.4",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-images": "^0.5.19",
    "react-photo-gallery": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "css-build": "node-sass --omit-source-map-url sass/mystyles.scss src/css/mystyles.css",
    "css-watch": "npm run css-build -- --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}